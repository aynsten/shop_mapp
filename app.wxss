/**app.wxss**/

@import "colorui/main.wxss";
@import "colorui/icon.wxss";

page {
    background:#f4f4f4;
}
.scrollPage {
  height: 100vh;
}
.checkbox-wrapper:before{
    content:"" !important;
    width:0 !important;
    height:0 !important;
}
view.scrollPage{
    display: flex;
    flex-direction: column;
    align-items: left;
}
cu-custom{
    position: sticky;
    top: 0;
    z-index: 999;
}
button.action{
    background: transparent;
}
button.not-button{
    background: transparent;
    border: 0;
    line-height: inherit;
    font-size:inherit;
}
button.not-button::after{
    border: 0;
}

.cuIcon-blank{
    background-size:cover;
}
.cuIcon-blank::before{
    content:"\e6d7";
    color: transparent;
}

.dbline{
    line-height:1.1em;
}
.cu-list .cu-item .content .icon-xs:before{
    font-size:24rpx;
}
.cu-list .cu-item .content .icon-df:before{
    font-size:28rpx;
}
.cu-list .cu-item .content .icon-lg:before{
    font-size:32rpx;
}
.cu-list .cu-item .content .icon-xl:before{
    font-size:36rpx;
}
.cu-list .cu-item .content>.flex .icon-xxl:before{
    font-size:50rpx;
}

.cu-list.grid>.cu-item text.item-title{
    margin-top:20rpx;
}
.list-radius{
    margin:20rpx;
    border-radius:10rpx;
}
.icon-hasbg .cu-item{
    position:relative;
}
.icon-hasbg .cu-item:before{
    content:"";
    display:block;
    width:80rpx;
    height:80rpx;
    border-radius: 100%;
    background-color:#f5f1eb;
    position: absolute;
    left:50%;
    margin-left:-40rpx;
    top:15rpx;
}

.bg-light-gold{
    background-color:#f5f1eb;
}
.text-gold{
    color:#a26c48 !important;
}
.text-golddark{
    color:#805033 !important;
}


.card-header{
    padding:10rpx 20rpx;
}
.card-header .flex{
    align-items: center;
}
.card-content{
    padding:20rpx;
}


.text-light {
  font-weight: 300;
}

.loading{
    text-align: center;
    line-height: 100rpx;
    height: 100rpx;
    color: #999;
}

.emptytip {
    text-align: center;
    line-height: 2em;
    padding: 100rpx 0;
    color: #999;
}

.text_over {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: block;
    width: 100%;
}



.block .block-inner{
    margin: 30rpx;
    background: #fff;
    border-radius:20rpx;
    padding:20rpx 0;
    box-shadow: 0 2rpx 10rpx rgba(0,0,0,.05)
}

.block-ttl {
    padding: 15rpx 20rpx;
    font-size: 32rpx;
    font-weight: bold;
    color:#333;
}
.block-inner .block-ttl {
    padding: 5rpx 20rpx;
    margin-bottom:30rpx;
}
.block-ttl .fr{
    font-weight:normal;
    color:#999;
}

.block-content {
    padding: 20rpx;
}
.block-content .cu-list.grid.no-border{
    padding:0;
}
.goods-block .block-content {
    padding: 0 0 0 30rpx;
}
.goods-block .imgitem{
    margin-right:30rpx;
    margin-bottom:30rpx;
    width:calc(33.33%-20rpx);
}
.goods-block .ttlbox {
    height:2.6em;
    line-height:1.2em;
    padding-top:.3em;
    overflow: hidden;
}

.goods-block .price{
    font-weight:bold;
    color:#d00;
    display:block;
}
.goods-block .price::first-letter{
    font-size:20rpx;
    font-weight:normal;
}
.goods-block .rightinfo{
    float:right;
    font-size:22rpx;
    color: #999;
    text-decoration: line-through;
}

.imgbox{
    width:100%;
    padding-top:100%;
    height:0;
    overflow: hidden;
    position:relative;
}

.imgbox image{
    position:absolute;
    width:100%;
    height:100%;
    left:0;
    top:0;
    border-radius:10rpx;
}


number-input{
    width:200rpx;
}

/*富文本样式*/
.detail-content{
    padding:0 20rpx;
}
rich-text{
    font-size:28rpx;
    line-height:1.5em;
}
rich-text .tag_p{
    text-indent:2em;
}
rich-text .tag_img{
    max-width: 100%;
}
rich-text .tag_p .tag_img{
    margin-left:-2em;
}
.detail-content rich-text .tag_img{
    border-radius: 5rpx;
    margin:10rpx 0;
    box-shadow: 1rpx 1rpx 5rpx rgba(0,0,0,.1);
}
.detail-content rich-text .tag_p .tag_img{
    margin-left:-2em;
}


.ucenter-bg .block-inner{
    margin-top:0;
    background:url(data:image/png;base64,)  left bottom no-repeat #ede9c9;
    background-size:cover;
    position: relative;
}

.secfoot{
    position: fixed;
    width: 100%;
    bottom: 100rpx;
    z-index: 1023;
}

.secfoot .submit{
    flex:1 !important;
}
.secfoot .action:first-child{
    padding-left:20rpx;
    flex:0 0 auto;
}
