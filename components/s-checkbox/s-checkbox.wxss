/* Components/s-checkbox/s-checkbox.wxss */
.s-checkbox{
  display:flex;
  flex-direction: row;
}
.s-checkbox .label{
  margin-left:10rpx;
  line-height:40rpx;
}

.s-checkbox .control{
  width:40rpx;
  height:40rpx;
  background:#f5f5f5;
  border-radius: 200rpx;
  background:url("data:image/png;base64,") center center no-repeat #f5f5f5;
  border:1rpx #ddd solid;
  box-sizing: border-box;
  background-size: 65%;
}

.checked .control{
  background-image:url("data:image/png;base64,");
  border-color:#ffa200;
  background-color:#ffa200;
}