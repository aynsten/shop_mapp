/* pages/member/index.wxss */

.block-head{
  padding:25rpx 20rpx;
  font-size:28rpx;
  border-bottom:1rpx #f0f0f2 solid;
}

.block-content{
  display: flex;
  flex-direction: row;
}

.member-info{
  width:100%;
  padding:40rpx 0 0 0;
  background:url('data:image/png;base64,') center bottom no-repeat #fff;
  background-size: cover;
}
.member-info .member-info-row{
    display: flex;
  flex-direction: row;
  width:100%;
}
.member-info .m-avatar{
    width:120rpx;
    padding-left:30rpx;
    flex:0 0 auto;
    position: relative;
}
.m-avatar .gender{
    position: absolute;
    width:30rpx;
    height:30rpx;
    right:10rpx;
    top:10rpx;
    border-radius:100rpx;
    box-shadow: 1rpx 1rpx 5rpx rgba(0,0,0,.2)
}
.member-info .avatar{
  width:120rpx;
  height:120rpx;
   border-radius:200rpx; 
}
.member-info .m-info{
  flex-grow: 1;
  width:50%;
  padding-left:20rpx;
  margin-right:20rpx;
}
.m-info .icon{
  width:24rpx;
  height:24rpx;
  background:transparent;
}
.m-info .inforow{
  display:flex;
  flex-direction: row;
  margin-top:24rpx;
  line-height:24rpx;
}
.m-info .inforow .mobile{
	font-size: 24rpx;
}
.m-info .inforow text{
  margin-right:10rpx;
  height:24rpx;
}
.m-info .inforow image{
  margin-right:10rpx;
}
.m-info .inforow navigator{
  height:24rpx;
  line-height:24rpx;
  color:#9b9b9b;
}
.member-info .m-toolbar{
    padding-right:30rpx;
}
.member-info .m-toolbar .icon{
    width:40rpx;
    height:40rpx;
}

.statblock{
    display:flex;
    flex-direction: row;
    width:100%;
    margin:40rpx auto 0 auto;
}
.stat-item{
  width:27%;
  flex-grow: 1;
  align-items: center;
  text-align: center;
  display: flex;
  flex-direction: column;
  padding:20rpx 0;
	justify-content: space-between;
}
.statblock .item-value{
  color:#333;
  font-weight:bold;
  line-height:50rpx;
  font-size:28rpx;
}
.statblock .logo{
  width:60rpx;
  height:60rpx;
  border-radius:10rpx;
}
.statblock .item-title{
  white-space: nowrap;
  width:100%;
  overflow: hidden;
  text-overflow: ellipsis;
	font-size: 24rpx;
    line-height:32rpx;
	color:#555;
}

.block-head .float_right{
  color:#9b9b9b;
}

.orderinfo .block-content{
  padding:40rpx 0;
}
.order-item{
  width:18%;
  flex-grow: 1;
  display:flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.order-item .number{
  position:absolute;
  left:50%;
  top:-15rpx;
  border-radius:100rpx;
  background:#f00;
  color:#fff;
  text-align: center;
  width:30rpx;
  height:30rpx;
  font-size:16rpx;
  line-height:32rpx;
}
.order-item .icon{
  width:40rpx;
  height:40rpx;
  margin-bottom:15rpx;
  background:transparent;
}
.order-item .item-title{
	font-size:24rpx;
	color: #6b6b6b;
}


.toolsinfo .block-content{
  padding:40rpx 0;
}
.tool-item{
  width:22%;
  flex-grow: 1;
  display:flex;
  flex-direction: column;
  align-items: center;
}
.tool-item image{
  width:40rpx;
  height:40rpx;
  background:transparent;
  margin-bottom:15rpx;
}
.tool-item .item-title{
	font-size:24rpx;
	color:#6b6b6b;
}

.avatarWrap{
	position: relative;
	margin-left: 40rpx;
	margin-right: 20rpx;
}
.avatarWrap .gender{
	position: absolute;
	top:0;
	right: 0;
	width: 32rpx;
	height: 32rpx;
	
}
.user-info .iconvip{
	width: 48rpx;
	height: 48rpx;
	margin-bottom: -4rpx
}
.user-info .user_vip{
	align-items: flex-end;
}
.user-info .mobile{
	font-size: 24rpx;
	color: #9b9b9b;
}

.member-bg1{
    background-image: url('data:image/png;base64,')
}
.member-bg2{
    background-image: url('data:image/png;base64,')
}
.member-bg3{
    background-image: url('data:image/png;base64,')
}